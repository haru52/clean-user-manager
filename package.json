{
  "name": "clean-architecture-example",
  "version": "0.1.0",
  "description": "Example TypeScript project of Clean Architecture.",
  "private": true,
  "engines": {
    "node": "^16.17.0",
    "npm": "^8.15.0"
  },
  "main": "index.js",
  "scripts": {
    "lint": "run-s --continue-on-error lint:*",
    "lint:credentials": "[ \"$CI\" = 'true' ] || secretlint \"**/*\"",
    "lint:md": "markdownlint . \".?*/**/*.{md,markdown}\"",
    "lint:prettier": "prettier --check .",
    "format": "run-s --continue-on-error format:*",
    "format:md": "npm run lint:md -- --fix",
    "format:prettier": "prettier --write .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "cz",
    "cm": "npm run commit",
    "prepare": "[ \"$CI\" = 'true' ] || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haru52/clean_architecture_example.git"
  },
  "keywords": [
    "template"
  ],
  "author": "haru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haru52/clean_architecture_example/issues"
  },
  "homepage": "https://github.com/haru52/clean_architecture_example#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@secretlint/secretlint-rule-preset-recommend": "^5.2.3",
    "@types/node": "^18.7.8",
    "commitizen": "^4.2.5",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": ">=0.32.2 <1.0.0",
    "npm-check-updates": "^16.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "secretlint": "^5.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
