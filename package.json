{
  "name": "@haru52/user-mgr",
  "version": "0.1.0",
  "description": "User management system. This is an example TypeScript project of Clean Architecture.",
  "publishConfig": {
    "access": "public"
  },
  "private": true,
  "engines": {
    "node": "^16.17.0",
    "npm": "^8.15.0"
  },
  "files": [
    "dist"
  ],
  "bin": "dist/index.js",
  "scripts": {
    "clean": "run-s --continue-on-error clean:*",
    "clean:dist": "rm -rf dist",
    "clean:db": "rm -f user_mgr.db",
    "lint": "run-s --continue-on-error lint:*",
    "lint:credentials": "[ \"$CI\" = 'true' ] || secretlint \"**/*\"",
    "lint:md": "markdownlint . \".?*/**/*.{md,markdown}\"",
    "lint:prettier": "prettier --check .",
    "lint:es": "eslint --cache .",
    "format": "run-s --continue-on-error format:*",
    "format:md": "npm run lint:md -- --fix",
    "format:prettier": "prettier --write .",
    "format:es": "npm run lint:es -- --fix",
    "prebuild": "npm run clean:dist",
    "build": "tsc",
    "test": "jest",
    "commit": "cz",
    "cm": "npm run commit",
    "prepare": "[ \"$CI\" = 'true' ] || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haru52/clean_architecture_example.git"
  },
  "keywords": [
    "Clean Architecture"
  ],
  "author": "haru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haru52/clean_architecture_example/issues"
  },
  "homepage": "https://github.com/haru52/clean_architecture_example#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@secretlint/secretlint-rule-preset-recommend": "^5.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^28.1.8",
    "@types/node": "<16.18.0",
    "@types/sqlite3": "<5.1.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "commitizen": "^4.2.5",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "28.x",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": ">=0.32.2 <1.0.0",
    "npm-check-updates": "^16.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "secretlint": "^5.2.4",
    "semantic-release": "^19.0.5",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "sqlite3": "^5.0.11"
  }
}
